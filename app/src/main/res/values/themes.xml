<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="OrangeButton" parent="@style/Widget.AppCompat.Button.Colored">

        <item name="colorButtonNormal">#F7705A</item>
        <item name="colorControlHighlight">#F12C0B</item>
    </style>

    <style name="MyCheckBox" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">#F7705A</item>
        <item name="colorControlActivated">#F12C0B</item>
    </style>

    <style name="RegrSign" parent="@style/Widget.AppCompat.Button.Colored">

        <item name="colorButtonNormal">#F3F3F3</item>
        <item name="colorControlHighlight">#F12C0B</item>
    </style>

    <style name="GreyButton" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">#E7E7E7</item>
        <item name="colorControlHighlight">#5E332D</item>
    </style>

    <style name="Theme.App.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlNormal">#F06751</item>
        <item name="colorControlActivated">#FA6A52</item>
        <item name="colorControlHighlight">#F7705A</item>
    </style>
    <style name="AppTheme.FloatingAccentButtonOverlay" >
        <item name="colorAccent">#FA4526</item>
        <item name="colorBackgroundFloating">#F86767</item>
        <item name="colorControlHighlight">#F86767</item>
        <item name="colorControlActivated">#F86767</item>



    </style>
</resources>